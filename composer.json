{
    "name": "oligriffiths/gunit",
    "description": "API testing suite that sits on top of Guzzle + PHPUNit",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "phpunit/phpunit": "^5.0"
    },
    "require-dev": {
        "brianium/paratest": "^0.15",
        "guzzlehttp/oauth-subscriber": "^0.3"
    },
    "suggest": {
        "brianium/paratest": "Facilitates parallel test running",
        "guzzlehttp/oauth-subscriber": "Allows for OAuth1 authentication through GUnit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oli Griffiths",
            "email": "oli.griffiths@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OliGriffiths\\GUnit\\": "src/"
        }
    }
}
